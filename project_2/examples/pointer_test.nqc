VOID SWAPFUNC( REF INT px , REF INT py )
BEGIN
	DEREF px := DEREF px + DEREF py;
	DEREF py := DEREF py - DEDEREF px;
	DEREF px := DEREF px - DEREF py;
END

INT MAIN( )
BEGIN
	INT flag_err;
	INT x;
	INT y;
	REF INT px;
	REF INT py;

	flag_err := 0;
	x := 5;
	y := 2;
	px := &x;
	py := &y;

	SWAPFUNC( px , py );

	writei(x);
	writes("n");
	writei(y);
	writes("n");

	MAIN := flag_err;
END

