VOID SWAPFUNC( REF I64 px , REF I64 py )
BEGIN
	DEREF px := DEREF px + DEREF py;
	DEREF py := DEREF py - DEDEREF px;
	DEREF px := DEREF px - DEREF py;
END

I64 MAIN( )
BEGIN
	I64 flag_err;
	I64 x;
	I64 y;
	REF I64 px;
	REF I64 py;

	flag_err := 0;
	x := 5;
	y := 2;
	px := &x;
	py := &y;

	SWAPFUNC( px , py );

	writei(x);
	writes("n");
	writei(y);
	writes("n");

	MAIN := flag_err;
END

