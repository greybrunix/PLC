I64 ackermann( I64 m, I64 n )
BEGIN
	IF (m=0) BEGIN ackermann := n+1; END
	ELSE BEGIN
		IF (n=0) BEGIN ackermann := ackermann( m-1,1 ); END
		ELSE BEGIN ackermann := ackermann( m-1 , ackermann( m,n-1 ) ); END
	END
END

I64 MAIN( )
BEGIN
	I64 m; I64 n; I64 res;
	m:=2;n:=3;
	res := ackermann( m,n);
	MAIN:=0;
END

